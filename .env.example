# Maxwell Discord Selfbot Configuration

# Required Configuration
# IMPORTANT: Store these securely using a secret manager, not in plain text
DISCORD_USER_TOKEN=your_discord_user_token_here
GOOGLE_API_KEY=your_google_ai_api_key_here

# Optional Configuration
DISCORD_USER_ID=your_discord_user_id_here
ADMIN_USER_ID=your_admin_user_id_here

# Security Settings
TOKEN_ENCRYPTION_KEY=your_32_character_encryption_key_here
ENABLE_TOKEN_VALIDATION=true
SESSION_TIMEOUT=3600

# AI Provider Settings
GOOGLE_AI_MODEL=models/gemma-3-27b-it

# NVIDIA NIM (Optional - fallback provider)
NVIDIA_NIM_API_KEY=your_nvidia_nim_api_key_here
NVIDIA_NIM_BASE_URL=https://integrate.api.nvidia.com/v1
NVIDIA_NIM_MODEL=google/gemma-3-27b-it
NVIDIA_NIM_MAX_TOKENS=32768
NVIDIA_NIM_TEMPERATURE=0.7

# Logging Configuration
LOG_LEVEL=info